{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug Android",
			"program": "${workspaceRoot}/.vscode/launchReactNative.js",
			"type": "reactnative",
			"request": "launch",
			"platform": "android",
            "runtimeArgs": [ "--nolazy" ],
            "cwd": "${workspaceRoot}",
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/.vscode/"
		},
		{
			"name": "Debug iOS",
			"program": "${workspaceRoot}/.vscode/launchReactNative.js",
			"type": "reactnative",
			"request": "launch",
			"platform": "ios",
            "target": "iPhone 5s",
            "runtimeArgs": [ "--nolazy" ],
            "cwd": "${workspaceRoot}",
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/.vscode/.react/"
		}
	]
}